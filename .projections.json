{
    "README.md": {
        "type": "readme"
    },
    "src/*.rs": {
        "type": "src",
        "alternate": "tests/{}.rs"
    },
    "tests/*.rs": {
        "type": "test",
        "alternate": "src/{}.rs",
        "dispatch": "cargo test {}"
    },
    "Cargo.toml": {
        "type": "config"
    }
}
